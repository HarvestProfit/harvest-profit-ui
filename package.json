{
  "name": "harvest-profit-ui",
  "version": "2.0.0",
  "description": "Harvest Profit UI Components",
  "main": "dist/index.js",
  "repository": "https://github.com/HarvestProfit/harvest-profit-ui",
  "author": "Jaryd Krishnan",
  "contributors": [
    "Jake Humphrey"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "babel src -d dist --source-maps",
    "clean": "rm -rf ./dist",
    "lint": "eslint --config .eslintrc.json src/",
    "report-coverage": "coveralls < ./coverage/lcov.info",
    "start": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "test": "jest",
    "test-coverage": "jest test/ --coverage"
  },
  "files": [
    "LICENSE.md",
    "README.md",
    "scss",
    "dist",
    "images"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/!(index).{js,jsx}"
    ],
    "setupFilesAfterEnv": ["./setupTests.js"],
    "timers": "fake",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "\\.(svg|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js"
    }
  },
  "keywords": [
    "harvest",
    "profit",
    "react",
    "component",
    "components",
    "react-component",
    "ui"
  ],
  "peerDependencies": {
    "react": "^0.14.9 || ^15.3.0 || ^16.0.0 || ^17.0.0",
    "react-dom": "^0.14.9 || ^15.3.0 || ^16.0.0 || ^17.0.0"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.21",
    "lodash.isfinite": "^3.3.2",
    "lodash.snakecase": "^4.1.1",
    "lodash.tonumber": "^4.0.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-sortable-hoc": "^1.11.0",
    "react-transition-group": "^2.9.0",
    "reactstrap": "^8.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7",
    "@babel/core": "^7",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/preset-react": "^7",
    "@babel/preset-stage-2": "^7",
    "@babel/runtime": "^7.0.0",
    "@webpack-blocks/core": "^2.0.0",
    "babel-eslint": "^10",
    "babel-jest": "^24.8.0",
    "babel-preset-airbnb": "^4.0.1",
    "coveralls": "^3.0.0",
    "enzyme": "^3.10.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^24.8.0",
    "jest-enzyme": "^4.0.1",
    "postcss-smart-import": "^0.7.5",
    "react-dom": "^17.0.2",
    "react-styleguidist": "^9.0.0",
    "webpack": "^4.0.0",
    "webpack-blocks": "^2.0.0"
  }
}
