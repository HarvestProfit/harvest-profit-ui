{
  "name": "harvest-profit-ui",
  "version": "1.0.0-alpha.10",
  "description": "Harvest Profit UI Components",
  "main": "dist/index.js",
  "repository": "https://github.com/HarvestProfit/harvest-profit-ui",
  "author": "Jaryd Krishnan",
  "contributors": [
    "Jake Humphrey"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "babel src -d dist",
    "clean": "rm -rf ./dist",
    "lint": "eslint --config .eslintrc.json src/",
    "report-coverage": "coveralls < ./coverage/lcov.info",
    "start": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "test": "jest test/ --coverage"
  },
  "files": [
    "LICENSE.md",
    "README.md",
    "scss",
    "dist"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/!(index).{js,jsx}"
    ],
    "setupTestFrameworkScriptFile": "./setupTests.test.js",
    "timers": "fake",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "keywords": [
    "harvest",
    "profit",
    "react",
    "component",
    "components",
    "react-component",
    "ui"
  ],
  "peerDependencies": {
    "react": "^0.14.9 || ^15.3.0 || ^16.0.0",
    "react-dom": "^0.14.9 || ^15.3.0 || ^16.0.0"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "lodash.isfinite": "^3.3.2",
    "lodash.snakecase": "^4.1.1",
    "lodash.tonumber": "^4.0.3",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-transition-group": "^2.2.1",
    "reactstrap": "^5.0.0-alpha.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "7",
    "babel-polyfill": "^6.26.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^3.0.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "postcss-smart-import": "^0.7.5",
    "react-dom": "^16.0.0",
    "react-styleguidist": "^6.0.31",
    "webpack": "^3.8.1",
    "webpack-blocks": "^1.0.0-rc.2"
  }
